 } else
 if (climate == CLIMATE_ARCTIC) {
  replace replace_tunnels_arctic (2421, "gfx/EMPTY.png") {
   template_tunnels_empty()
  }
  alternative_sprites (replace_tunnels_arctic, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx/TUNNELS.png") {
   template_tunnels(2)
  }
  alternative_sprites (replace_tunnels_arctic, ZOOM_LEVEL_IN_2X, BIT_DEPTH_32BPP, "gfx/TUNNELSx2.png") {
   template_tunnels_zoom_x2(2)
  }
 }
